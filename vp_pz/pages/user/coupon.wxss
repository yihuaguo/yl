/* vp_pz/pages/user/coupon.wxss */
.couponContainer {
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: center;
  background: #F2F2F2;
  padding: 30rpx;
  min-height: 100vh;
  gap: 30rpx;
}

.card {
  width: 100%;
  height: 266rpx;
  border-radius: 20rpx;
  background-color: red;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
  overflow: hidden;
}

view.cardNow {
  background-image: linear-gradient(270deg, #09A7AE 0%, #09B783 100%);
}

view.cardExpired {
  background-image: linear-gradient(270deg, #CCCCCC 0%, #DEDEDE 100%);
}

view.cardExpired .header .left .title .tag {
  color: #999999;
}

view.cardExpired .header .left .title .price {
  color: #999999;
}

view.cardExpired .header .left .label {
  color: #999999;
}

view.cardExpired .header .right .title {
  color: #999999;
}

view.cardExpired .header .right .timeBox .title {
  color: #999999;
}

view.cardExpired .header .right .timeBox .time {
  color: #999999;
}

view.cardExpired .header .right {
  background-image: url(data:image/png;base64,);
  background-position: right;
  background-size: contain;
  background-repeat: no-repeat;
}

.card .header {
  height: 190rpx;
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  box-sizing: border-box;
  padding: 30rpx 40rpx;
}

.card .header .left {
  width: 240rpx;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  border-right: 1px dashed #fff;
}

.card .header .left .title {
  display: flex;
  justify-content: center;
  align-items: baseline;
  margin-right: 15px;
}

.card .header .left .title .tag {
  font-family: PingFangSC-Semibold;
  font-size: 34rpx;
  color: #FFFFFF;
}

.card .header .left .title .price {
  font-family: PingFangSC-Semibold;
  font-size: 62rpx;
  color: #FFFFFF;
  font-weight: 600;
}

.card .header .left .label {
  font-family: PingFangSC-Regular;
  font-size: 22rpx;
  color: #FFFFFF;
  margin-right: 10px;
  margin-bottom: 15px;
}

.card .header .right {
  margin-left: 50rpx;
  flex: 1;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: flex-start;
}

.card .header .right .title {
  font-family: PingFangSC-Medium;
  font-size: 36rpx;
  color: #FFFFFF;
}

.card .header .right .timeBox {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: flex-start;
}

.card .header .right .timeBox .title {
  font-family: PingFangSC-Regular;
  font-size: 22rpx;
  color: #FFFFFF;
}

.card .header .right .timeBox .time {
  font-family: PingFangSC-Medium;
  font-size: 24rpx;
  color: #FFFFFF;
}

.card .footer {
  height: 76rpx;
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 14rpx 30rpx;
  background-color: #fff;
  box-sizing: border-box;
}

.card .footer .title {
  font-family: PingFangSC-Regular;
  font-size: 19rpx;
  color: #999999;
}

.card .footer .bt {
  padding: 7rpx 20rpx;
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  border-radius: 20rpx;
  color: #FFFFFF;
}

view.now {
  background: #0BB584;
}

.expired {
  background: #B3B3B3;
}

.received {
  background: transparent;
  color: #666666 !important;
}