.card-type{border:2rpx solid #5196ef;background:#d7f2ff;border-radius:10rpx;padding:40rpx 20rpx 20rpx 20rpx;text-align:center;margin:0 15rpx 0 30rpx}
.card-type-icon image{width:100rpx;height:100rpx;}
.card-type-name {margin:20rpx 0;}
.card-type-name text{font-weight:bold;font-size:36rpx;color:#5196ef;}
.card-type-intro{text-align:left;}
.card-type-button{margin-top:20rpx}


.block-tt{text-align:center;margin:40rpx 0}
.block-tt text{font-weight:bold;color:#535353;position:relative;}
.block-tt text::before{content:" ";position:absolute;width:20rpx;top:16rpx;left:-30rpx;border-top:2rpx solid #535353}
.block-tt text::after{content:" ";position:absolute;width:20rpx;top:16rpx;right:-30rpx;border-top:2rpx solid #535353}



.article-box{margin:30rpx;border:1rpx solid #EEEEEE;border-radius:10rpx;}         
.article-tt{background:#F8F8F8;padding:30rpx;font-weight:bold;border-radius:10rpx 10rpx 0 0; }       
.article-tt image{display:block;width:30rpx;height:30rpx;margin-right:15rpx}
.article-cells{margin-top:0;background:none;padding:15rpx 0;}
.article-cells::before{display:none;}
.article-cells .weui-cell{padding:15rpx 30rpx;}
.article-cells .weui-cell::before{display:none;}

.link-box{margin:30rpx;border:1rpx solid #EEEEEE;border-radius:10rpx;}         
.link-tt{background:#F8F8F8;padding:30rpx;font-weight:bold;border-radius:10rpx 10rpx 0 0; }       
.link-tt image{display:block;width:30rpx;height:30rpx;margin-right:15rpx}
.link-cells{margin-top:0;background:none;}
.link-cells::before{display:none;}

.link-cells .weui-cell{padding:30rpx;}
.link-icon{display:block;width:80rpx;height:80rpx;margin-right:20rpx}
.link-name{font-weight:bold}
.link-intro{margin-top:5rpx;font-size:24rpx;color:#BBBBBB;}

