/* vp_pz/pages/staff/check.wxss */
page{background:#f4f4f4;}

.check-box{margin:30rpx;border:1rpx solid #2d7aff;border-radius:10rpx;overflow:hidden;}
.check-box::before{display:none;}
.check-box .weui-cell{padding:30rpx;}
.check-box .weui-cell::before{display:none;}
.tags-cell::before{display:none;}

.images:after{content:"";display:block;height:0;line-height:0;clear:both;visibility:hidden;}
.images-item{float:left;height:145rpx;width:145rpx;margin:0 10rpx 10rpx 0;}
.images-item image{width:100%;height:100%}




.check-list{margin:30rpx;background:#FFFFFF;border-radius:10rpx;overflow:hidden;}
.check-list::before{display:none;}
.check-item{padding:30rpx;-webkit-box-align:stretch;-webkit-align-items:stretch;align-items:stretch;}
.check_line{margin-bottom:5rpx;}
.check-images{white-space:nowrap;width:580rpx;overflow:hidden}
.check-images image{display:inline-block;height:150rpx;width:150rpx;margin-right:10rpx;}

.text-loc{font-size:24rpx;color:#666666;display:inline-block;padding:0 0 0 30rpx;background:url(data:image/png;base64,) no-repeat left center;background-size:30rpx;}

