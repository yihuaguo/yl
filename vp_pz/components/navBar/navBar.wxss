.nav{position:fixed;width:100%;top:0;left:0;z-index:2}
.navbar{
    position: relative
}
.back-icon, .home-icon{
    width: 32px;
    height: 100%;
    position: absolute;
    transform: translateY(-50%);
    top: 50%;
    display: flex;
}
.back-icon{
    left: 10px;
}
.home-icon{
    left: 44px
}
.back-icon cover-image{
    width: 32px;
    height: 32px;
    margin: auto;
}
.home-icon cover-image{
    width: 20px;
    height: 20px;
    margin: auto;
}
.back-icon image{
    width: 32px;
    height: 32px;
    margin: auto;
}
.home-icon image{
    width: 20px;
    height: 20px;
    margin: auto;
}
.nav-icon{
    position: absolute;
    transform: translate(-50%, -50%);
    left: 50%;
    top: 50%;
}

.nav-title{text-align:center;font-size: 0;font-weight: bold;}